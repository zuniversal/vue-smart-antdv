{
  "name": "vue-smart-antdv",
  "version": "1.0.10",
  "description": "vue ant-design-vue element form smart register custom component",
  "type": "module",
  "author": "tvsqzyb",
  "license": "MIT",
  "keywords": [
    "vue",
    "ant-design-vue",
    "element",
    "form",
    "smart",
    "custom",
    "component"
  ],
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/zuniversal/vue-smart-antdv"
  },
  "files": [
    "assets",
    "build",
    "dist",
    "lib",
    "examples",
    "packages",
    "src"
  ],
  "main": "lib/smart.umd.js",
  "scripts": {
    "buildm": "vue-cli-service build --target lib --name smart --dest lib --entry ./packages/main.js",
    "build": "vue-cli-service build --target lib --name smart --dest lib --entry ./packages/index.js",
    "vbuild": "vite build --outDir dist/index",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "vite": "^3.2.3",
    "vite-plugin-compression": "^0.5.1",
    "yorkie": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^3.2.12",
    "@element-plus/icons": "^0.0.11",
    "element-plus": "^2.2.25",
    "vue": "^3.0.0"
  }
}
